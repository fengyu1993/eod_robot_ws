  <link name="right_camera_depth_frame"/>

  <joint name="right_camera_depth_optical_joint" type="fixed">
    <origin rpy="-1.57079632679 0 -1.57079632679" xyz="0 0 0"/>
    <parent link="right_camera_depth_frame"/>
