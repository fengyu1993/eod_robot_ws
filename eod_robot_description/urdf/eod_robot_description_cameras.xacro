    <parent link="right_camera_color_frame"/>
    <child link="right_camera_color_optical_frame"/>
  </joint>

  <link name="right_camera_color_optical_frame"/>

<!--left_camera-->
<joint name="left_base_camera_joint" type="fixed">
	<origin xyz="-0.027 0.0815 -0.0515" rpy="0.18 0.00 1.57079632679"/>
	<parent link="Arm_L6_link_add"/>
