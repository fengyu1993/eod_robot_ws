  </joint>

  <link name="right_camera_right_ir_frame"/>

  <joint name="right_camera_right_ir_optical_joint" type="fixed">
    <origin rpy="-1.57079632679 0 -1.57079632679" xyz="0 0 0"/>
    <parent link="right_camera_right_ir_frame"/>
    <child link="right_camera_right_ir_optical_frame"/>
  </joint>

  <link name="right_camera_right_ir_optical_frame"/>

  <!-- camera color joints and links -->
  <joint name="right_camera_color_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0.015 0"/>
    <parent link="right_camera_depth_frame"/>
    <child link="right_camera_color_frame"/>
  </joint>

  <link name="right_camera_color_frame"/>

  <joint name="right_camera_color_optical_joint" type="fixed">
